#include <stdio.h>
// Aayush Mitra 10025

int main() {
    int a, b;
    printf("Enter value of a: ");
    scanf("%d", &a);
    printf("Enter value of b: ");
    scanf("%d", &b);
    printf("Numbers from %d to %d:\n", a, b);
    for (int i = a; i <= b; i++) {
        printf("%d\n", i);
    }
    return 0;
}

// Enter value of a: 2
// Enter value of b: 7
// Numbers from 2 to 7:
// 2
// 3
// 4
// 5
// 6
// 7


// === Code Execution Successful ===
